package com.mall.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.mall.domain.MemberVO;
import com.mall.mapper.AdMemMapper;

import lombok.Setter;

@Service
public class AdMemServiceImpl implements AdMemService{

	@Setter(onMethod_ = {@Autowired})
	private AdMemMapper adMemMapper;
	
	@Override
	public List<MemberVO> memberList(String mbsp_id) {
		// TODO Auto-generated method stub
		return adMemMapper.memberList(mbsp_id);
	}

}
